{
  "name": "wyzie-lib",
  "version": "2.4.0",
  "icon": "https://i.postimg.cc/L5ppKYC5/cclogo.png",
  "license": "MIT",
  "keywords": [
    "wyzie-subs",
    "wyzie subs",
    "wyzie api",
    "subtitle",
    "search",
    "library",
    "typescript",
    "vite",
    "fast",
    "scraper",
    "subtitle scraper",
    "open subtitles"
  ],
  "type": "module",
  "main": "./lib/main.js",
  "types": "./lib/main.d.ts",
  "files": [
    "./lib"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/main.d.ts",
        "default": "./lib/main.js"
      },
      "require": {
        "types": "./lib/main.d.ts",
        "default": "./lib/main.cjs"
      },
      "umd": {
        "types": "./lib/main.d.ts",
        "default": "./lib/main.umd.cjs"
      },
      "es": {
        "types": "./lib/main.d.ts",
        "default": "./lib/main.js"
      },
      "cjs": {
        "types": "./lib/main.d.ts",
        "default": "./lib/main.cjs"
      },
      "iife": {
        "types": "./lib/main.d.ts",
        "default": "./lib/main.iife.js"
      },
      "amd": {
        "types": "./lib/main.d.ts",
        "default": "./lib/main.amd.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itzcozi/wyzie-lib.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "test": "npx vitest",
    "format": "prettier --log-level warn --write \"{src/**/*.{ts},*.{ts,js,html,css,json,md,xml}}\"",
    "preview": "vite preview"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vite": "^4.5.14",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^2.1.9"
  }
}
